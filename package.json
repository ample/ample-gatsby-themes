{
  "name": "gatsby-theme-ample-components",
  "version": "0.0.1",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "develop": "yarn run test:generate-output && start-storybook -p 6006",
    "clean": "rm -rf storybook-static",
    "build": "yarn run test:generate-output && build-storybook --quiet -c .storybook",
    "lint": "eslint -c .eslintrc.js . && stylelint --config stylelint.config.js 'src/**/*.scss'",
    "test": "jest src",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "test:update": "jest src --updateSnapshot"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "@storybook/addon-a11y": "^5.3.8",
    "@storybook/addon-docs": "^5.3.8",
    "@storybook/addon-essentials": "^5.3.8",
    "@storybook/addon-jest": "^5.3.17",
    "@storybook/preset-scss": "^1.0.2",
    "@storybook/react": "next",
    "classnames": "^2.2.6",
    "css-loader": "^3.4.2",
    "gatsby": "^2.18.12",
    "gatsby-react-router-scroll": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "sass": "^1.26.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-preset-gatsby": "^0.2.28",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "prettier": "^2.0.2",
    "prettier-stylelint": "^0.4.2",
    "react-test-renderer": "^16.12.0",
    "stylelint": "^13.2.1",
    "stylelint-formatter-pretty": "^2.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.16.0"
  }
}
